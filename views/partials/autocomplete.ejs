        <script>
      console.log($(window).width())
let testArr = {}
<% let bigBrews = brews%>
<% bigBrews.sort((a, b)=> { %>
<%	if(a.name < b.name) { return -1; } %>
<%    if(a.name > b.name) { return 1; } %>
<%    return 0; %>
<% }) %>
<% bigBrews.forEach((brewery) => { %>
     testArr["<%=brewery.name%>"] = null
    <% }) %>
        
        $('input.autocomplete').autocomplete({
      data: 
        testArr,
        onAutocomplete: () => {
          console.log($(window).width)
          if($(window).width() >975) {
            $('#brewSearch').submit()
          } else {
          $('.littleBrew').submit()
          }
        }
    }
    
    
    );
        </script>